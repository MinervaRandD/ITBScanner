Public Class CountryClass

    Public countryName As String
    Public countryCode As String

    Public Sub New(ByVal inputCountryName As String, ByVal inputCountryCode As String)

        countryName = inputCountryName
        countryCode = inputCountryCode

    End Sub

    Public Overrides Function ToString() As String

        Return countryName & " (" & countryCode & ")"

    End Function

End Class
