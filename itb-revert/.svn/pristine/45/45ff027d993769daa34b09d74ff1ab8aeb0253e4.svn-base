      <wsdl:input message="tns:GetCarriersRequest" />
      <wsdl:output message="tns:GetCarriersResponse" />
    </wsdl:operation>
    <wsdl:operation name="GetCities">
      <wsdl:documentation>get list of cities for given scanner</wsdl:documentation>
      <wsdl:input message="tns:GetCitiesRequest" />
      <wsdl:output message="tns:GetCitiesResponse" />
    </wsdl:operation>
    <wsdl:operation name="GetCarrierCity">
      <wsdl:documentation>get list of cities for each carrier</wsdl:documentation>
      <wsdl:input message="tns:GetCarrierCityRequest" />
