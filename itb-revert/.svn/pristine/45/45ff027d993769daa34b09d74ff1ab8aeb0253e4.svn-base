      <wsdl:input message="tns:GetFileRequest" />
      <wsdl:output message="tns:GetFileResponse" />
    </wsdl:operation>
    <wsdl:operation name="GetFlights">
      <wsdl:documentation>get list of flights for given carrier departing from or arriving at given location during given dates</wsdl:documentation>
      <wsdl:input message="tns:GetFlightsRequest" />
      <wsdl:output message="tns:GetFlightsResponse" />
    </wsdl:operation>
    <wsdl:operation name="GetHotBags">
      <wsdl:documentation>get list of hot bags, i.e. bags to be transferred to another flight in limited time, to be unloaded from given flight</wsdl:documentation>
      <wsdl:input message="tns:GetHotBagsRequest" />
      <wsdl:output message="tns:GetHotBagsResponse" />
    </wsdl:operation>
    <wsdl:operation name="GetMissingBags">
      <wsdl:documentation>get list of missing bags, i.e. bags unloaded locally and yet to be loaded to to given flight</wsdl:documentation>
      <wsdl:input message="tns:GetMissingBagsRequest" />
      <wsdl:output message="tns:GetMissingBagsResponse" />
    </wsdl:operation>
